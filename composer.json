{
    "name" : "jarobe42/xero-bundle",
    "type" : "symfony-bundle",
    "description" : "Symfony2 bundle for wrapping ",
    "keywords": ["symfony2", "xero"],
    "homepage": "https://github.com/jarobe42/XeroBundle",
    "authors" : [{
        "name" : "James Robertson",
        "email" : "jim@jarobe.com"
    }],
    "require": {
      "php": ">=5.5.0",
      "symfony/framework-bundle": "~2.8.8",
      "calcinai/xero-php": "~1.3.3"
    },
    "require-dev": {
      "phpunit/phpunit": "^4.8.24"
    },
    "autoload" : {
      "psr-0" : {
        "Jarobe\\XeroBundle" : ""
      },
      "exclude-from-classmap": [
        "/Tests/"
      ]
    },
    "target-dir" : "Jarobe/XeroBundle"
}
